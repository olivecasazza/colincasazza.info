{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/colincasazza.info/src",
  "styleIncludePaths": ["libs/theme/src/css"],
  "namedInputs": {
    "default": ["{projectRoot}/**/*"]
  },
  "targets": {
    "build": {
      "executor": "nx-vue3-vite:build-app",
      "options": {
        "dist": "apps/colincasazza.info/dist"
      },
      "outputs": ["dist/apps/colincasazza.info"],
      "dependsOn": ["^build"]
    },
    "deploy": {
      "dependsOn": ["build", "default"],
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "firebase deploy"
        ],
        "cwd": "apps/colincasazza.info"
      }
    },
    "serve": {
      "executor": "nx-vue3-vite:dev-server"
    },
    "e2e": {
      "executor": "nx-vue3-vite:cypress",
      "options": {
        "cypressConfig": "apps/colincasazza.info/cypress.json",
        "testingType": "component"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/colincasazza.info/**/*.{js,jsx,ts,tsx,vue}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/colincasazza.info"],
      "options": {
        "jestConfig": "apps/colincasazza.info/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
